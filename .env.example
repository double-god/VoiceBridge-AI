# ============================================
# PostgreSQL 数据库配置
# ============================================
POSTGRES_USER=nainong
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=nainong

# ============================================
# MinIO 对象存储配置
# ============================================
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=your_minio_password_here

# ============================================
# JWT 安全密钥 (请使用强随机字符串)
# ============================================
JWT_SECRET=your_jwt_secret_key_here

# ============================================
# LLM API 配置 (阿里云通义千问)
# ============================================
AI_AGENT_LLM_API_URL=https://dashscope.aliyuncs.com/compatible-mode/v1/chat/completions
LLM_API_KEY=your_dashscope_api_key_here

# ============================================
# TTS 代理配置 (可选)
# ============================================
# 如果 Edge TTS 无法访问,可配置代理
# 例: http://127.0.0.1:7890 或 socks5://127.0.0.1:1080
TTS_PROXY=

